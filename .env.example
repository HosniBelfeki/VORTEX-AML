# AWS Configuration
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your_aws_access_key_here
AWS_SECRET_ACCESS_KEY=your_aws_secret_key_here

# LandingAI Configuration
# Get your API key from: https://landing.ai/
LANDING_AI_API_KEY=your_landing_ai_api_key_here

# Database Configuration
DATABASE_URL=postgresql://user:password@localhost:5432/aml_db

# External APIs (Optional)
OFAC_API_KEY=your_ofac_api_key_here
REFINITIV_API_KEY=your_refinitiv_api_key_here

# Application Settings
DEBUG=True
LOG_LEVEL=INFO
SECRET_KEY=your-secret-key-change-in-production

# Service Mode (AUTO, REAL, or MOCK)
SERVICE_MODE=AUTO